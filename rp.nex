/[ \t]/  { /* Skip blanks and tabs. */ }

/add|ADD/ { return ADD }
/lw|LW/ { return LW }
/sw|SW/ { return SW }
/beq|BEQ/ { return BEQ }
/addfp|ADDFP/ { return ADDFP }
/nop|NOP/ { return NOP }

/,/ { return COMA }
/\[/ { return AC }
/\]/ { return CC }

/-[0-9]+/ { lval.n,_ = strconv.Atoi(yylex.Text()); return NUM }
/[0-9]+/ { lval.n,_ = strconv.Atoi(yylex.Text()); return NUM }

/./ {  }

